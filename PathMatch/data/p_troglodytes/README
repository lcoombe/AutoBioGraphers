## Extract high confidence interactions >700 (as recommended by stringdb)
## Done in different location so file paths will not work
cat ../graphmatch/pan_data/graphmatch/protein.links_raw.txt |perl -ne 'chomp; @a=split (/\s+/); if($a[2]>700) {print "$_\n";} ' > pt_protein.links_scoreOver700.txt
python ../graphmatch/filterSequences.py pt_protein.links_scoreOver700.txt ../graphmatch/pan_data/graphmatch/protein.sequences.v10_raw.fa > pt_protein.sequences_scoreOver700.fa

## Make BLAST DB on GSC server:
/projects/rwarren_prj/solexa/HLAminer_dev/HLAminer_v1.3dev/bin/formatdb -p T -i pt_protein.sequences_scoreOver700.fa

## Run BLAST
/projects/rwarren_prj/solexa/HLAminer_dev/HLAminer_v1.3dev/bin/blastall -p blastp -m8 -d pt_protein.sequences_scoreOver700.fa -i ../query_path.fa >blast_700.out

## Reformat into corr file input
cat blast_700.out|perl -ne 'chomp; @a=split(/\t/); if($a[10] < 0.00001){print "$a[0]\t $a[1]\t$a[10]\n";} ' > pt_corr700.txt

## Reformat into corr file input
python ../remove_duplicates.py pt_corr700.txt > out && mv out pt_corr700.txt 
