## Extract high confidence interactions >700 (as recommended by stringdb)
## Done in different location so file paths will not work
cat ../graphmatch/m_musculus/mm_protein.links_raw.txt|perl -ne 'chomp; @a=split(/\s+/); if($a[2]>700){print "$_\n";} ' > mm_protein.links_scoreOver700.txt
python ../graphmatch/filterSequences.py mm_protein.links_scoreOver700.txt ../graphmatch/m_musculus/mm_protein.sequences_raw.fa > mm_protein.sequences_scoreOver700.fa

## Reformat into input graph file
cat mm_protein.links_scoreOver700.txt |perl -ne 'chomp; @a=split(/\s+/); print "$a[0]\t$a[1]\n"; ' > graph_input_700

## Make BLAST DB on GSC server:
/projects/rwarren_prj/solexa/HLAminer_dev/HLAminer_v1.3dev/bin/formatdb -p T -i mm_protein.sequences_scoreOver700.fa 

## Run BLAST
/projects/rwarren_prj/solexa/HLAminer_dev/HLAminer_v1.3dev/bin/blastall -p blastp -m8 -d mm_protein.sequences_scoreOver700.fa -i ../query_path.fa >blast_700.out

## Reformat into corr file input
cat blast_700.out|perl -ne 'chomp; @a=split(/\t/); if($a[10] < 0.00001){print "$a[0]\t$a[1]\t$a[10]\n";} ' > mm_corr700.txt                             
## Remove duplicates - keep the entry with lower e-value
python ../remove_duplicates.py mm_corr700.txt > out && mv out mm_corr700.txt
